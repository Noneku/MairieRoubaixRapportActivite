{% block field_rapportActiviteFile %}
<pre>
    {{ dump(entity.instance.rapportActiviteFileName) }}
</pre>
    {% if entity.instance.rapportActiviteFileName %}
        <a href="{{ asset('./public/rapportDownload/' ~ entity.instance.rapportActiviteFileName) }}" download>
            <button>Télécharger</button>
        </a>
    {% else %}
        <span>Aucun fichier</span>
    {% endif %}
{% endblock %}
